      font-style: normal;
      font-display: swap;
  }

  @font-face {
      font-family: 'ProximaNovaRg';
      src: url({{ "ProximaNova-Regular.woff2" | file_url }}) format('woff2'),
          url({{ "ProximaNova-Regular.woff" | file_url }}) format('woff');
      font-weight: normal;
      font-style: normal;
      font-display: swap;
  }

  @font-face {
      font-family: 'ProximaNovaRg';
      src: url({{ "ProximaNova-Bold.woff2" | file_url }}) format('woff2'),
